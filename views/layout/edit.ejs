<h2>Update Book</h2>
<form action="/update/<%= myChosenBook.id %>?_method=PUT" method="post">
  <input type="text" name=author value="<%= myChosenBook.author %>" placeholder="author" />
  <input type="text" name=title value="<%= myChosenBook.title %>" placeholder="title" />
  <input type="text" name=image_url value="<%= myChosenBook.image_url %>" placeholder="image_url" />
  <input type="text" name=isbn value="<%= myChosenBook.isbn %>" placeholder="isbn" />
  <input type="text" name=description value="<%= myChosenBook.description %>" placeholder="description" />
  <button type="submit">Update Details</button>
</form>

